{
  "roots": [
    "app/javascript"
  ],
  "moduleDirectories": [
    "node_modules",
    "app/javascript"
  ],
  "transform": {
    "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
    ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
  },
  "transformIgnorePatterns": [
    "/node_modules(?![\\\\/]vue-awesome[\\\\/])/"
  ],
  "restoreMocks": true,
  "testURL": "http://localhost:3000/"
}
