<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
<url>
  <loc>http://www.triunfei.com/</loc>
  <changefreq>daily</changefreq>
  <priority>1.00</priority>
</url>
<url>
  <loc>http://blog.triunfei.com</loc>
  <changefreq>daily</changefreq>
  <priority>0.85</priority>
</url>
<url>
  <loc>http://blog.triunfei.com/sobre/</loc>
  <changefreq>daily</changefreq>
  <priority>0.85</priority>
</url>
<% BlogPost.all.each do |blog_post|%>
  <url>
    <loc><%= blog_post.url%></loc>
    <changefreq>daily</changefreq>
    <priority>0.50</priority>
  </url>
<% end %>
<% Bank.all.each do |bank| %>
  <url>
    <loc><%= bank_url(bank)%></loc>
    <changefreq>daily</changefreq>
    <priority>0.85</priority>
  </url>
<% end %>
<url>
  <loc>http://www.triunfei.com/contas-corrente</loc>
  <changefreq>daily</changefreq>
  <priority>0.85</priority>
</url>
<url>
  <loc>http://www.triunfei.com/cartoes-de-credito</loc>
