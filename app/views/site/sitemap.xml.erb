  <priority>0.85</priority>
</url>
<url>
  <loc>http://www.triunfei.com/emprestimos</loc>
  <changefreq>daily</changefreq>
  <priority>0.85</priority>
</url>
<% LandingPage.all.each do |landing| %>
<url>
  <loc><%= landing_url(id: landing.slug) %></loc>
  <changefreq>daily</changefreq>
  <priority>0.85</priority>
</url>
<% end %>
<% Bank.joins(:cards).distinct.all.each do |bank|%>
  <url>
    <loc>
      <%= filter_cards_url(path: bank.slug) %>
    </loc>
    <changefreq>daily</changefreq>
    <priority>0.85</priority>
